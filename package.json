    "cordova-plugin-compat": "1.2.0",
    "cordova-plugin-console": "1.1.0",
    "cordova-plugin-contacts": "2.1.0",
    "cordova-plugin-crosswalk-webview": "2.4.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "4.3.3",
    "cordova-plugin-file-transfer": "1.5.1",
    "cordova-plugin-geolocation": "4.0.1",
    "cordova-plugin-inappbrowser": "3.0.0",
    "cordova-plugin-mauron85-background-geolocation": "2.3.6",
    "cordova-plugin-nativeaudio": "3.0.9",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sms-plugin": "git+https://github.com/cordova-sms/cordova-sms-plugin.git",
    "cordova-sqlite-storage": "^0.7.11",
    "cordova.plugins.diagnostic": "3.1.1",
    "lodash": "^4.17.11",
    "phonegap": "^8.0.0",
    "phonegap-plugin-barcodescanner": "^6.0.8"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-android-permissions": {},
      "cordova-plugin-background-mode": {},
      "cordova-plugin-app-version": {},
      "cordova-plugin-console": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-mauron85-background-geolocation": {
        "GOOGLE_PLAY_SERVICES_VERSION": "11+",
        "ANDROID_SUPPORT_LIBRARY_VERSION": "23+",
        "ICON": "@mipmap/icon",
        "SMALL_ICON": "@mipmap/icon",
        "ACCOUNT_NAME": "@string/app_name",
        "ACCOUNT_LABEL": "@string/app_name",
        "ACCOUNT_TYPE": "$PACKAGE_NAME.account",
        "CONTENT_AUTHORITY": "$PACKAGE_NAME",
        "ALWAYS_USAGE_DESCRIPTION": "This app always requires location tracking"
      },
      "cordova-plugin-nativeaudio": {},
      "cordova-plugin-file": {},
      "cordova-plugin-file-transfer": {},
      "cordova-plugin-buildinfo": {},
      "cordova-custom-config": {},
      "cordova.plugins.diagnostic": {},
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": " ",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": " "
      },
      "cordova-plugin-contacts": {},
      "cordova-plugin-calendar": {},
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-compat": {},
      "phonegap-plugin-barcodescanner": {
        "CAMERA_USAGE_DESCRIPTION": " "
      },
      "cordova-sqlite-storage": {},
      "cordova-sms-plugin": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
